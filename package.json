{
  "name": "corporategiftslandingpage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "postbuild": "npx react-snap",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "inlineCss": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "ignore": [
      "mailto:*",
      "tel:*",
      "https://*",
      "http://*"
    ],
    "include": [
      "/thank-you",
      "/quote"
    ]
  },
  "browserslist": [
    "> 0.01%"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.14",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lenis": "^1.3.11",
    "motion": "^12.23.24",
    "react": "^19.1.1",
    "react-datepicker": "^8.8.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-icon": "^1.0.0",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-snap": "^1.23.0",
    "react-parallax": "^3.5.2",
    "react-router": "^7.9.4",
    "swiper": "^12.0.2",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-lazy-load-image-component": "^1.6.4",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
